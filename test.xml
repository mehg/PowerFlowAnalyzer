<?xml version="1.0" encoding="ASCII" standalone="yes"?>
<power_flow_case xmlns="http://www.mehg.net/schema/PowerFlowAnalyzer">
    <model-db>
        <modelClass baseType="bus" label="Bus">
            <parameter ID="BUS_I" type="integer" label="Bus" description="Bus number" restriction="bus-number" empty="true"/>
            <parameter ID="BUS_TYPE" type="integer" label="Type" description="Bus type (1 = PQ, 2 = PV, 3 = ref, 4 = isolated)" restriction="list" empty="true">
                <options value="1" label="PQ" ID="PQ"/>
                <options value="2" label="PV" ID="PV"/>
                <options value="3" label="Reference" ID="REF"/>
                <options value="4" label="Isolated" ID="ISO"/>
            </parameter>
            <parameter ID="BASE_KV" type="double" label="&lt;html&gt;Base&lt;sub&gt;kV&lt;/sub&gt;&lt;/html&gt;" description="Base voltage (kV)" empty="true"/>
            <parameter ID="LATTITUDE" type="double" label="Lattitude" description="" empty="true"/>
            <parameter ID="LONGITUDE" type="double" label="Longitude" description="" empty="true"/>
        </modelClass>
        <modelClass baseType="branch" label="Branch">
            <parameter ID="F_BUS" type="integer" label="F_BUS" description="From Bus" restriction="bus-number"/>
            <parameter ID="T_BUS" type="integer" label="T_BUS" description="To Bus" restriction="bus-number"/>
            <parameter ID="R" type="double" label="Resistance">
                <display increment="1.0E-4"/>
            </parameter>
            <parameter ID="BR_STATUS" type="integer" label="Initial branch status" restriction="list" defaultValue="1">
                <options value="1" label="in-service" ID="1"/>
                <options value="0" label="out-of-service" ID="0"/>
            </parameter>
            <parameter ID="susceptance" value="1.5e-8"/>
            <modelClass label="Cable"/>
            <model modelNumber="1" label="Unknown Branch"/>
        </modelClass>
    </model-db>
</power_flow_case>
