<?xml version="1.0" encoding="US-ASCII" standalone="yes"?>
<power_flow_case xmlns="http://www.mehg.net/schema/PowerFlowAnalyzer">
    <model-db>
        <modelClass ID="conf" label="Configuration">
            <modelClass ID="viewer" label="Viewer">
                <parameter ID="TITLE" type="text" label="Title" empty="true">
                    <display/>
                </parameter>
                <parameter ID="POSITION" type="text" label="Position" restriction="list" value="bottom" empty="false">
                    <option value="left" label="Left" ID="LEFT"/>
                    <option value="bottom" label="Bottom" ID="BOTTOM"/>
                    <option value="right" label="Right" ID="RIGHT"/>
                    <option value="free" label="Free" ID="FREE"/>
                    <display/>
                </parameter>
                <modelClass ID="table" label="Table">
                    <model ID="type_filter" label="Data Table">
                        <parameter ID="ELEMENT_FILTER" type="text" label="Element filter" restriction="model or class ID" empty="true">
                            <display/>
                        </parameter>
                    </model>
                </modelClass>
            </modelClass>
        </modelClass>
    </model-db>
</power_flow_case>
