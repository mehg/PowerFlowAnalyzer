<?xml version="1.0" encoding="US-ASCII" standalone="yes"?>
<power_flow_case xmlns="http://www.mehg.net/schema/PowerFlowAnalyzer">
    <model-db>
        <modelClass baseType="bus" ID="bus" label="Bus">
            <parameter ID="BUS_I" type="integer" label="Bus" description="Bus number" restriction="bus-number" empty="true">
                <display/>
            </parameter>
            <parameter ID="BUS_TYPE" type="integer" label="Type" description="Bus type (1 = PQ, 2 = PV, 3 = ref, 4 = isolated)" restriction="list" empty="true">
                <option value="1" label="PQ" ID="PQ"/>
                <option value="2" label="PV" ID="PV"/>
                <option value="3" label="Reference" ID="REF"/>
                <option value="4" label="Isolated" ID="ISOLATED"/>
                <display/>
            </parameter>
            <parameter ID="LATTITUDE" type="double" label="Lattitude" description="" empty="true"/>
            <parameter ID="LONGITUDE" type="double" label="Longitude" description="" empty="true"/>
            <parameter ID="PD" type="double" label="Real Power Demand (MW)" empty="true">
                <display/>
            </parameter>
            <parameter ID="QD" type="double" label="Reactive Power Demand (MVAr)" empty="true">
                <display/>
            </parameter>
            <parameter ID="GS" type="double" label="Shunt Conductance" description="MV demanded at V = 1.0 p.u." empty="true">
                <display/>
            </parameter>
            <parameter ID="BS" type="double" label="Shunt Susceptance" description="MVAr injected at V = 1.0 p.u." empty="true">
                <display/>
            </parameter>
            <parameter ID="BUS_AREA" type="integer" label="Area" description="Bus Area" empty="true">
                <display/>
            </parameter>
            <parameter ID="VM" type="double" label="Voltage Magnitude" empty="true">
                <display decimalFormatPattern="#.###"/>
            </parameter>
            <parameter ID="VA" type="double" label="Voltage Angle (degrees)" empty="true">
                <display/>
            </parameter>
            <parameter ID="BASE_KV" type="double" label="Base-kV" description="Base voltage (kV)" empty="true">
                <display/>
            </parameter>
            <parameter ID="VMAX" type="double" label="Max. voltage (p.u.)" description="Maximum voltage magnitude" empty="true">
                <display/>
            </parameter>
            <parameter ID="VMIN" type="double" label="Min. voltage (p.u.)" description="Minimum voltage magnitude" empty="true">
                <display/>
            </parameter>
            <parameter ID="LAM_P" type="double" label="LaM P (u/MW)" description="Lagrange multiplier on real power mismatch" empty="true">
                <display/>
            </parameter>
            <parameter ID="LAM_Q" type="double" label="LaM Q (u/MVAr)" description="Lagrange multiplier on reactive power mismatch" empty="true">
                <display/>
            </parameter>
            <parameter ID="MU_VMAX" type="double" label="Mult Vmax (u/p.u.)" description="Kuhn-Tucker multiplier on upper voltage limit (u/p.u.)" empty="true">
                <display/>
            </parameter>
            <parameter ID="MU_VMIN" type="double" label="Mult Vmin (u/p.u.)" description="Kuhn-Tucker multiplier on lower voltage limit (u/p.u.)" empty="true">
                <display/>
            </parameter>
            <model ID="unknown" label="Unknown Bus"/>
        </modelClass>
        <modelClass baseType="branch" ID="branch" label="Branch">
            <parameter ID="F_BUS" type="integer" label="From Bus" description="From Bus" restriction="bus-number" empty="true">
                <display/>
            </parameter>
            <parameter ID="T_BUS" type="integer" label="To Bus" description="To Bus" restriction="bus-number" empty="true">
                <display/>
            </parameter>
            <parameter ID="R" type="double" label="Resistance" empty="true">
                <display increment="1.0E-4"/>
            </parameter>
            <parameter ID="BR_STATUS" type="integer" label="Initial branch status" restriction="list" empty="false">
                <option value="1" label="in-service" ID="1"/>
                <option value="0" label="out-of-service" ID="0"/>
                <display/>
            </parameter>
            <parameter ID="susceptance" label="Susceptance" value="1.5e-8">
                <display/>
            </parameter>
            <modelClass label="Cable"/>
            <model ID="unknown" label="Unknown Branch"/>
        </modelClass>
        <modelClass ID="generator" label="Generator">
            <parameter ID="GEN_BUS" type="integer" label="Bus Number" restriction="bus-number" empty="true">
                <display/>
            </parameter>
            <parameter ID="PG" type="double" label="Real Power Output (MW)" empty="true">
                <display/>
            </parameter>
            <parameter ID="QG" type="double" label="Reactive Power Output (MVAr)" empty="true">
                <display/>
            </parameter>
            <parameter ID="QMAX" type="double" label="Maximum Reactive Power Output (MVAr)" empty="true">
                <display/>
            </parameter>
            <parameter ID="QMIN" type="double" label="Minimum Reactive Power Output (MVAr)" empty="true">
                <display/>
            </parameter>
            <parameter ID="VG" type="double" label="Voltage Magnitude Setpoint (p.u.)" empty="true">
                <display/>
            </parameter>
            <parameter ID="MBASE" type="double" label="Total MVA Base" description="Total MVA Base of machine, defaults to baseMVA" empty="true">
                <display/>
            </parameter>
            <parameter ID="GEN_STATUS" type="integer" label="Machine Status" restriction="list">
                <option value="1" label="in-service" ID="1"/>
                <option value="0" label="out-of-service" ID="0"/>
                <display/>
            </parameter>
            <model ID="unknown" label="Unknown Generator"/>
        </modelClass>
    </model-db>
</power_flow_case>
